# Тестовая конфигурация базы данных
# Скопируйте этот файл в .env.test и настройте под вашу среду

# Настройки MySQL для тестов
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_password_here

# URL тестовой базы данных
DATABASE_URL=mysql://root:your_password_here@localhost:3306/utki_test

# Настройки приложения для тестов
NODE_ENV=test
PORT=3001

# OAuth настройки (если нужны для тестов)
# OAUTH_CLIENT_ID=your_client_id
# OAUTH_CLIENT_SECRET=your_client_secret
# OAUTH_REDIRECT_URI=http://localhost:3001/api/oauth/callback





